<script type="text/javascript">
  var submenu = document.getElementById('submenu');
  var parentmenu = document.getElementById('parentmenu');
  parentmenu.addEventListener('click',function() {
    if(submenu.style.display == 'none'){
      submenu.style.display = 'block';
    }else{
      submenu.style.display = 'none';
    }
  });
  var submenu2 = document.getElementById('submenu2');
  var parentmenu2 = document.getElementById('parentmenu2');
  parentmenu2.addEventListener('click',function() {
    if(submenu2.style.display == 'none'){
      submenu2.style.display = 'block';
    }else{
     submenu2.style.display = 'none';
    }
  });
</script>

